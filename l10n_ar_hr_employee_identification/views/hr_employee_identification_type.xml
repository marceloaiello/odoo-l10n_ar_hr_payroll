<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="hr_employee_identification_type_view_form" model="ir.ui.view">
        <field name="model">hr.employee.identification.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="hr_employee_identification_type_view_search" model="ir.ui.view">
        <field name="model">hr.employee.identification.type</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
            </search>
        </field>
    </record>
    <record id="hr_employee_identification_type_view_tree" model="ir.ui.view">
        <field name="model">hr.employee.identification.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    <act_window id="action_hr_employee_identification_type" name="Identification types" res_model="hr.employee.identification.type" view_mode="tree,form" />
    <menuitem id="menu_hr_employee_identification_type" action="action_hr_employee_identification_type" parent="hr.menu_human_resources_configuration" />
</odoo>
